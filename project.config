[project]
	description = Working repository for EA-976 undergrad class on Software Engineering at University of Campinas
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/ra137540
[access "refs/for/refs/*"]
	read = group user/ra137540
	push = group user/ra137540
	label-Code-Review = -2..+2 group user/ra137540
	label-Verified = -1..+1 group user/ra137540
	rebase = group user/ra137540
	removeReviewer = group user/ra137540
	submit = group user/ra137540
[access "refs/heads/*"]
	read = group user/ra137540
	create = group user/ra137540
	push = +force group user/ra137540
	pushMerge = group user/ra137540
[access "refs/tags/*"]
	push = +force group user/ra137540
	pushTag = group user/ra137540
	pushSignedTag = group user/ra137540
